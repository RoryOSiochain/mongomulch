

//Data Model 


// Array of schemas 


[ SchemaName

	[
	  field  [
               {property (which can be data type, validation, reference to another schema, embedded schema)}
			   	   
		     ]

	]

]



[Schema1, Schema2]

Schema1 = {
	name: nameofSchema
	fields: [{
		fieldName : "",
	 	fieldType: String|Number|Boolean|Ref|Obj,
	  	options:{
		  			isUnique: true,
		  			isRequired:true,
		  			default :'created',
		  			enum
	  			}
	  	hint: ""
	}]
}

